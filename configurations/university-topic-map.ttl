#####################################
# Queries
#####################################

<https://linked.opendata.cz/resource/knowledge-graph-browser/expansion-query/uk/podřazená-pracoviště> a browser:ExpansionQuery ;
  dct:title "Podřazená pracoviště" ;
  browser:hasDataset <https://linked.opendata.cz/resource/knowledge-graph-browser/dataset/uk/pracoviště> ;
  browser:query """PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX schema: <http://schema.org/>
PREFIX buk: <https://linked.opendata.cz/ontology/knowledge-graph-browser/uk/>
PREFIX browser: <https://linked.opendata.cz/ontology/knowledge-graph-browser/>

CONSTRUCT {
  ?pracoviště a buk:Pracoviště ;
    buk:nadřazené-pracoviště ?node ;
    skos:prefLabel ?pracovištěLabel ;
    browser:class "pracoviste" .

  buk:nadřazené-pracoviště browser:class "broader" .
} WHERE {
  ?pracoviště skos:broader ?node ;
    skos:prefLabel ?pracovištěLabel .
  FILTER (LANG(?pracovištěLabel) = "cs")
}""" .

<https://linked.opendata.cz/resource/knowledge-graph-browser/expansion-query/uk/nadřazená-pracoviště> a browser:ExpansionQuery ;
  dct:title "Nadřazené pracoviště" ;
  browser:hasDataset <https://linked.opendata.cz/resource/knowledge-graph-browser/dataset/uk/pracoviště> ;
  browser:query """PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX schema: <http://schema.org/>
PREFIX buk: <https://linked.opendata.cz/ontology/knowledge-graph-browser/uk/>
PREFIX browser: <https://linked.opendata.cz/ontology/knowledge-graph-browser/>

CONSTRUCT {
  ?node buk:nadřazené-pracoviště ?pracoviště .
  ?pracoviště a buk:Pracoviště ;
    skos:prefLabel ?pracovištěLabel ;
    browser:class "pracoviste" .

  buk:nadřazené-pracoviště browser:class "broader" .
} WHERE {
  ?node skos:broader ?pracoviště .
  ?pracoviště skos:prefLabel ?pracovištěLabel .
  FILTER (LANG(?pracovištěLabel) = "cs")
}""" .

<https://linked.opendata.cz/resource/knowledge-graph-browser/expansion-query/uk/témata/pracoviště> a browser:ExpansionQuery ;
  dct:title "Témata pracoviště" ;
  browser:hasDataset <https://linked.opendata.cz/resource/knowledge-graph-browser/dataset/uk/témata> ;
  browser:query """PREFIX dct: <http://purl.org/dc/terms/>
PREFIX schema: <http://schema.org/>
PREFIX buk: <https://linked.opendata.cz/ontology/knowledge-graph-browser/uk/>
PREFIX browser: <https://linked.opendata.cz/ontology/knowledge-graph-browser/>

CONSTRUCT {
  ?node buk:zabývá-se-tématem ?keyword .
  ?keyword a buk:Téma ;
    dct:title ?keywordTitle ;
    browser:class "tema" .

  buk:zabývá-se-tématem browser:class "zabyvaSeTematem" .
} WHERE {
  ?předmět schema:provider ?node ;
    schema:keywords ?keyword .
  ?keyword dct:title ?keywordTitle .
}""" .

<https://linked.opendata.cz/resource/knowledge-graph-browser/expansion-query/uk/témata/téma> a browser:ExpansionQuery ;
  dct:title "Pracoviště tématu" ;
  browser:hasDataset <https://linked.opendata.cz/resource/knowledge-graph-browser/dataset/uk/témata> ;
  browser:query """PREFIX dct: <http://purl.org/dc/terms/>
PREFIX schema: <http://schema.org/>
PREFIX buk: <https://linked.opendata.cz/ontology/knowledge-graph-browser/uk/>
PREFIX browser: <https://linked.opendata.cz/ontology/knowledge-graph-browser/>

CONSTRUCT {
  ?pracoviště buk:zabývá-se-tématem ?node .
  ?pracoviště a buk:Pracoviště ;
    skos:prefLabel ?pracovištěLabel ;
    browser:class "pracoviste" .

  buk:zabývá-se-tématem browser:class "zabyvaSeTematem" .
} WHERE {
  ?předmět schema:provider ?pracoviště ;
    schema:keywords ?node .
  ?pracoviště skos:prefLabel ?pracovištěLabel .
  FILTER (LANG(?pracovištěLabel) = "cs")
}""" .

