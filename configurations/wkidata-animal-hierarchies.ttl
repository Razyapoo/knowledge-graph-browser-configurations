PREFIX wdab: <https://linked.opendata.cz/resource/vocabulary/knowledge-graph-browser/wikidata/animal-classification/>
PREFIX browser: <https://linked.opendata.cz/ontology/knowledge-graph-browser/>
CONSTRUCT {
  ?node rdfs:label ?nodeLabel ;
    schema:description ?nodeDescription ;
    wdt:P18 ?p18 .
} WHERE {
  ?node rdfs:label ?nodeLabel .
  optional { ?node schema:description ?nodeDescription . FILTER (lang(?nodeDescription) = "en")}
  optional { ?node wdt:P18 ?p18 . }

  FILTER(lang(?nodeLabel) = "en")

}""" .

