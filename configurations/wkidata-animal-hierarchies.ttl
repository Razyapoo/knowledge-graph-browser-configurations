#####################################
# Queries
#####################################

#####################################
# Expansion example
#
# Expansion of the node is defined by the query and dataset on which the query is executed. The expansion query is a SPARQL query which returns the neighbourhood of the node which shall be visualized after the expansion. It shall return all informaton necessary to display visually the nodes in the neighbourhood so that the client does not need to request the preview operation for each node in the neighbourhood.
#####################################

<https://linked.opendata.cz/resource/knowledge-graph-browser/expansion-query/animal-classification/kingdom/narrower> a browser:ExpansionQuery ;
  dct:title "Phyla of the Animal Kingdom"@en ;
  browser:hasDataset <https://linked.opendata.cz/resource/knowledge-graph-browser/dataset/wikidata> ;
  browser:query """PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX wdab: <https://linked.opendata.cz/resource/vocabulary/knowledge-graph-browser/wikidata/animal-classification/>
PREFIX browser: <https://linked.opendata.cz/ontology/knowledge-graph-browser/>
CONSTRUCT {
  ?childTaxon a wdab:taxon ;
    rdfs:label ?childTaxonLabel ;
    wdab:broader ?node ;
    browser:class "taxon", "animalVisualGroup", ?childTaxonClass .

  wdab:broader browser:class "broader" .
} WHERE {

  {
    ?childTaxon wdt:P105 wd:Q38348 ;
            wdt:P171 ?node ;
            rdfs:label ?childTaxonLabel .
  }
  UNION
  {
    ?childTaxon wdt:P105 wd:Q38348 ;
            wdt:P171/wdt:P171 ?node ;
            rdfs:label ?childTaxonLabel .
  }
  UNION
  {
    ?childTaxon wdt:P105 wd:Q38348 ;
            wdt:P171/wdt:P171/wdt:P171 ?node ;
            rdfs:label ?childTaxonLabel .
  }

  wd:Q38348 rdfs:label ?childTaxonClass .

  filter (lang(?childTaxonLabel) = "en")
  filter (lang(?childTaxonClass) = "en")

  SERVICE wikibase:label { bd:serviceParam wikibase:language "en". }

}""" .

<https://linked.opendata.cz/resource/knowledge-graph-browser/expansion-query/animal-classification/phylum/broader> a browser:ExpansionQuery ;
  dct:title "Kingdom of a phylum"@en ;
  browser:hasDataset <https://linked.opendata.cz/resource/knowledge-graph-browser/dataset/wikidata> ;
  browser:query """PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX wdab: <https://linked.opendata.cz/resource/vocabulary/knowledge-graph-browser/wikidata/animal-classification/>
PREFIX browser: <https://linked.opendata.cz/ontology/knowledge-graph-browser/>
CONSTRUCT {
  ?parentTaxon a wdab:taxon ;
    rdfs:label ?parentTaxonLabel ;
    browser:class "taxon", "animalVisualGroup", ?parentTaxonClass .

  ?node wdab:broader ?parentTaxon .

  wdab:broader browser:class "broader" .
} WHERE {

  ?node wdt:P171* ?parentTaxon .

  ?parentTaxon wdt:P105 wd:Q36732 ;
              rdfs:label ?parentTaxonLabel .

  wd:Q36732 rdfs:label ?parentTaxonClass .

  filter (lang(?parentTaxonLabel) = "en")
  filter (lang(?parentTaxonClass) = "en")

  SERVICE wikibase:label { bd:serviceParam wikibase:language "en". }

}""" .

<https://linked.opendata.cz/resource/knowledge-graph-browser/expansion-query/animal-classification/phylum/narrower> a browser:ExpansionQuery ;
  dct:title "Classes of a phylum"@en ;
  browser:hasDataset <https://linked.opendata.cz/resource/knowledge-graph-browser/dataset/wikidata> ;
  browser:query """PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX wdab: <https://linked.opendata.cz/resource/vocabulary/knowledge-graph-browser/wikidata/animal-classification/>
PREFIX browser: <https://linked.opendata.cz/ontology/knowledge-graph-browser/>
CONSTRUCT {
  ?childTaxon a wdab:taxon ;
    rdfs:label ?childTaxonLabel ;
    wdab:broader ?node ;
    browser:class "taxon", "animalVisualGroup", ?childTaxonClass .

  wdab:broader browser:class "broader" .
} WHERE {

  {
    select distinct ?childTaxon ?childTaxonLabel ?childTaxonClass ?node where {
      {
        ?childTaxon wdt:P105 wd:Q37517 ;
                    wdt:P171 ?node ;
                    rdfs:label ?childTaxonLabel .
      }
      UNION
      {
        ?childTaxon wdt:P105 wd:Q37517 ;
                    wdt:P171/wdt:P171 ?node ;
                    rdfs:label ?childTaxonLabel .
      }
      UNION
      {
        ?childTaxon wdt:P105 wd:Q37517 ;
                    wdt:P171/wdt:P171/wdt:P171 ?node ;
                    rdfs:label ?childTaxonLabel .
      }
      UNION
      {
        ?childTaxon wdt:P105 wd:Q37517 ;
                    wdt:P171/wdt:P171/wdt:P171/wdt:P171 ?node ;
                    rdfs:label ?childTaxonLabel .
      }
      UNION
      {
        ?childTaxon wdt:P105 wd:Q37517 ;
                    wdt:P171/wdt:P171/wdt:P171/wdt:P171/wdt:P171 ?node ;
                    rdfs:label ?childTaxonLabel .
      }
      UNION
      {
        ?childTaxon wdt:P105 wd:Q37517 ;
                    wdt:P171/wdt:P171/wdt:P171/wdt:P171/wdt:P171/wdt:P171 ?node ;
                    rdfs:label ?childTaxonLabel .
      }
      UNION
      {
        ?childTaxon wdt:P105 wd:Q37517 ;
                    wdt:P171/wdt:P171/wdt:P171/wdt:P171/wdt:P171/wdt:P171/wdt:P171 ?node ;
                    rdfs:label ?childTaxonLabel .
      }
      UNION
      {
        ?childTaxon wdt:P105 wd:Q37517 ;
                    wdt:P171/wdt:P171/wdt:P171/wdt:P171/wdt:P171/wdt:P171/wdt:P171/wdt:P171 ?node ;
                    rdfs:label ?childTaxonLabel .
      }
      UNION
      {
        ?childTaxon wdt:P105 wd:Q37517 ;
                    wdt:P171/wdt:P171/wdt:P171/wdt:P171/wdt:P171/wdt:P171/wdt:P171/wdt:P171/wdt:P171 ?node ;
                    rdfs:label ?childTaxonLabel .
      }
      UNION
      {
        ?childTaxon wdt:P105 wd:Q37517 ;
                    wdt:P171/wdt:P171/wdt:P171/wdt:P171/wdt:P171/wdt:P171/wdt:P171/wdt:P171/wdt:P171/wdt:P171 ?node ;
                    rdfs:label ?childTaxonLabel .
      }
      UNION
      {
        ?childTaxon wdt:P105 wd:Q37517 ;
                    wdt:P171/wdt:P171/wdt:P171/wdt:P171/wdt:P171/wdt:P171/wdt:P171/wdt:P171/wdt:P171/wdt:P171/wdt:P171 ?node ;
                    rdfs:label ?childTaxonLabel .
      }

      wd:Q37517 rdfs:label ?childTaxonClass .

      filter (lang(?childTaxonLabel) = "en")
      filter (lang(?childTaxonClass) = "en")

      SERVICE wikibase:label { bd:serviceParam wikibase:language "en". }
    }

  }
}""" .

