#####################################
# Queries
#####################################

<https://linked.opendata.cz/resource/knowledge-graph-browser/expansion-query/entity/has-out-relations> a browser:ExpansionQuery ;
  dct:title "Vztahy z"@cs ;
  browser:hasDataset <https://linked.opendata.cz/resource/knowledge-graph-browser/dataset/uksisdb> ;
  browser:query """PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX browser: <https://linked.opendata.cz/ontology/knowledge-graph-browser/>

CONSTRUCT {
  ?outRel a owl:ObjectProperty ;
    rdfs:domain ?node ;
    rdfs:range ?rngElem ;
    rdfs:label ?outRelLabel ;
    browser:class "relation" .

  rdfs:domain browser:class "domain" .
  rdfs:range browser:class "range" .

  ?rngElem a owl:Class ;
    rdfs:label ?rngLabel ;
    browser:class "entity" .
  
} WHERE {
  ?outRel rdfs:domain ?node ;
    rdfs:range ?rngElem ;
    rdfs:label ?outRelLabel .

  ?rngElem rdfs:label ?rngLabel .
}""" .

<https://linked.opendata.cz/resource/knowledge-graph-browser/expansion-query/entity/has-in-relations> a browser:ExpansionQuery ;
  dct:title "Vztahy do"@cs ;
  browser:hasDataset <https://linked.opendata.cz/resource/knowledge-graph-browser/dataset/uksisdb> ;
  browser:query """PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX browser: <https://linked.opendata.cz/ontology/knowledge-graph-browser/>

CONSTRUCT {
  ?inRel a owl:ObjectProperty ;
    rdfs:domain ?domElem ;
    rdfs:range ?node ;
    rdfs:label ?inRelLabel ;
    browser:class "relation" .

  rdfs:domain browser:class "domain" .
  rdfs:range browser:class "range" .

  ?domElem a owl:Class ;
    rdfs:label ?domLabel ;
    browser:class "entity" .
  
} WHERE {
  ?inRel rdfs:range ?node ;
    rdfs:domain ?domElem ;
    rdfs:label ?inRelLabel .

  ?domElem rdfs:label ?domLabel .
}""" .

<https://linked.opendata.cz/resource/knowledge-graph-browser/expansion-query/entity/has-attributes> a browser:ExpansionQuery ;
  dct:title "Atributy"@cs ;
  browser:hasDataset <https://linked.opendata.cz/resource/knowledge-graph-browser/dataset/uksisdb> ;
  browser:query """PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX browser: <https://linked.opendata.cz/ontology/knowledge-graph-browser/>

CONSTRUCT {
  ?attr a owl:DatatypeProperty ;
    rdfs:domain ?node ;
    rdfs:label ?attrLabel ;
    browser:class "attribute" .

  rdfs:domain browser:class "domain" .

} WHERE {
  ?attr rdfs:domain ?node ;
    rdfs:label ?attrLabel .
}""" .

